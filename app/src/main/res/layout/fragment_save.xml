<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".fragments.SaveFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_toLeftOf="@+id/fragment_save_top_right"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </LinearLayout>

        <LinearLayout
            android:layout_alignParentRight="true"
            android:id="@+id/fragment_save_top_right"
            android:layout_width="300dp"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:background="@color/colorWhite">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:id="@+id/save_layout_right_top"
                    android:orientation="vertical"
                    android:background="@color/colorWhite"
                    android:visibility="visible">

                    <com.tabyq.pos.tabqypos.customtext.CustomTextView
                        android:id="@+id/text_customer"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/status_bar_customer"
                        android:textSize="@dimen/size_10"
                        android:textColor="@color/color_lightgreytext"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:paddingBottom="10dp"
                        android:paddingTop="10dp"
                        android:gravity="center" >

                        <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                            android:layout_width="0dp"
                            android:layout_weight=".7"
                            android:layout_height="wrap_content"
                            android:id="@+id/fragment_save_online_order"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:text="@string/status_bar_online_order"
                            android:textAllCaps="true"
                            android:textSize="@dimen/size_10"
                            android:textColor="@color/colorBlack"
                            android:gravity="top|center_horizontal"
                            android:contentDescription="@string/nav_header_desc"
                            android:drawablePadding="@dimen/size_10"
                            android:drawableTop="@drawable/ic_online_order" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:visibility="gone"
                            android:layout_weight=".7"
                            android:id="@+id/fragment_walkin_order_online_layout">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:layout_gravity="center"
                                android:src="@drawable/x"
                                android:id="@+id/fragment_walkin_order_online_img"/>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_gravity="center">
                                <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Zamato"
                                    android:padding="10dp"
                                    android:textColor="@color/colorBlack"
                                    android:id="@+id/fragment_walkin_order_online_order"/>
                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="#6590790"
                                    android:paddingLeft="10dp"
                                    android:textColor="@color/colorBlack"
                                    android:id="@+id/fragment_walkin_order_online_order_id"/>

                            </LinearLayout>
                        </LinearLayout>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                            android:id="@+id/img_scan"
                            android:layout_width="0dp"
                            android:layout_weight=".7"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:text="@string/status_bar_scan"
                            android:textAllCaps="true"
                            android:textSize="@dimen/size_10"
                            android:textColor="@color/colorBlack"
                            android:gravity="top|center_horizontal"
                            android:contentDescription="@string/nav_header_desc"
                            android:drawablePadding="@dimen/size_10"
                            android:drawableTop="@drawable/ic_qr_code" />
                        <View
                            android:layout_width="1dp"
                            android:layout_height="@dimen/size_60"
                            android:background="@color/colorGrey"/>
                        <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                            android:id="@+id/img_search"
                            android:layout_width="0dp"
                            android:layout_weight=".7"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:text="@string/status_bar_search"
                            android:textAllCaps="true"
                            android:textSize="@dimen/size_10"
                            android:textColor="@color/colorBlack"
                            android:gravity="top|center_horizontal"
                            android:drawablePadding="@dimen/size_10"
                            android:contentDescription="@string/nav_header_desc"
                            android:drawableTop="@drawable/ic_magnifying_glass" />
                        <View
                            android:layout_width="1dp"
                            android:layout_height="@dimen/size_60"
                            android:background="@color/colorGrey"/>
                        <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                            android:id="@+id/img_adduser"
                            android:layout_width="0dp"
                            android:layout_weight=".7"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:text="@string/status_bar_add"
                            android:textAllCaps="true"
                            android:textSize="@dimen/size_10"
                            android:textColor="@color/colorBlack"
                            android:gravity="top|center_horizontal"
                            android:drawablePadding="@dimen/size_10"
                            android:contentDescription="@string/nav_header_desc"
                            android:drawableTop="@drawable/ic_add_user" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>


    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:visibility="visible">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="3dp">

                    <LinearLayout
                        android:padding="5dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:id="@+id/fragment_save_img_cusotm"
                                android:src="@drawable/profile_new"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_toRightOf="@+id/fragment_save_img_cusotm"
                                android:layout_centerVertical="true">

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="John"
                                    android:textSize="@dimen/extra_small_size"
                                    android:textColor="@color/colorBlack"
                                    android:paddingLeft="3dp"
                                    android:id="@+id/fragment_save_name"/>

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="3dp"
                                    android:textSize="10sp"
                                    android:layout_below="@+id/fragment_save_name"
                                    android:text="Not John? Create new Customer"
                                    android:layout_toRightOf="@+id/fragment_save_img_cusotm"
                                    android:id="@+id/fragment_save_not_name"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:paddingRight="5dp"
                                android:layout_alignParentRight="true">
                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Phone Number : "
                                    android:gravity="right"
                                    android:textSize="@dimen/extra_small_size"
                                    android:textColor="@color/colorlightBlack"
                                    android:paddingLeft="3dp" />
                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingLeft="3dp"
                                    android:gravity="right"
                                    android:textColor="@color/colorBlack"
                                    android:textSize="@dimen/small_size"
                                    android:layout_below="@+id/fragment_save_name"
                                    android:text="064-5664-4566"
                                    android:layout_toRightOf="@+id/fragment_save_img_cusotm"
                                    android:id="@+id/fragment_save_phone"/>
                            </LinearLayout>
                        </RelativeLayout>

                        <View
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/colorLightGrey"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="match_parent"
                            android:padding="5dp"
                            android:textColor="@color/colorBlack"
                            android:layout_height="wrap_content"
                            android:text="@string/fragment_save_txt_delivery"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="3dp"
                            android:background="@drawable/bg_light_grey">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="6dp"
                                android:layout_gravity="right">
                                <android.support.v7.widget.CardView
                                    android:layout_margin="5dp"
                                    app:cardCornerRadius="3dp"
                                    android:layout_gravity="right"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageView
                                        android:padding="3dp"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/ic_loc"/>
                                </android.support.v7.widget.CardView>
                                <android.support.v7.widget.CardView
                                    android:layout_margin="5dp"
                                    app:cardCornerRadius="3dp"
                                    android:layout_gravity="right"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <ImageView
                                        android:padding="3dp"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/ic_edit_pink"/>
                                </android.support.v7.widget.CardView>
                            </LinearLayout>

                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorBlack"
                                android:paddingLeft="15dp"
                                android:textSize="@dimen/extra_small_size"
                                android:paddingBottom="8dp"
                                android:text="E-38 , Noida Sector 63,\nUP, India "/>
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp">
                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorappPink"
                                android:drawablePadding="5dp"
                                android:gravity="center_vertical"
                                android:textSize="@dimen/extra_small_size"
                                android:text="@string/fragment_save_txt_delete"
                                android:id="@+id/fragment_save_txt_delete_address"
                                android:drawableLeft="@drawable/ic_delete_button"
                                android:drawableStart="@drawable/ic_delete_button" />

                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorWhite"
                                android:padding="5dp"
                                android:id="@+id/fragment_save_txt_add_address"
                                android:background="@drawable/btn_background_black"
                                android:drawablePadding="5dp"
                                android:textSize="@dimen/extra_small_size"
                                android:layout_alignParentRight="true"
                                android:gravity="center_vertical"
                                android:text="@string/fragment_save_txt_add_address"
                                android:drawableLeft="@drawable/ic_add"/>


                        </RelativeLayout>


                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/fragment_save_place_new_order"
                            android:padding="5dp"
                            android:layout_margin="8dp"
                            android:drawablePadding="5dp"
                            android:textSize="@dimen/extra_small_size"
                            android:textColor="@color/colorWhite"
                            android:layout_gravity="center"
                            android:drawableLeft="@drawable/ic_add"
                            android:background="@drawable/btn_backgroundpink"
                            android:text="@string/fragment_save_txt_place_order"/>

                    </LinearLayout>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="300dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="3dp"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Customer Insight"
                            android:padding="5dp"
                            android:textColor="@color/colorBlack"
                            android:layout_toRightOf="@+id/fragment_save_img_cusotm"
                            android:id="@+id/fragment_save_customer_insight"/>

                        <include layout="@layout/item_pager_save" />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_margin="5dp"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:background="@drawable/bg_light_grey">

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorappPink"
                                    android:text="BIRTHDAY"
                                    android:padding="3dp"
                                    android:textSize="@dimen/extra_small_size"
                                    android:gravity="center"
                                    android:textAllCaps="true"/>
                                <View
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/colorGrey"/>

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:padding="3dp"
                                    android:gravity="center"
                                    android:textSize="@dimen/extra_small_size"
                                    android:textColor="@color/colorlightBlack"
                                    android:text="Feb 27, 1995" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_margin="5dp"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="match_parent"
                                android:orientation="vertical"
                                android:background="@drawable/bg_light_grey">

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorappPink"
                                    android:text="AGE"
                                    android:padding="3dp"
                                    android:textSize="@dimen/extra_small_size"
                                    android:gravity="center"
                                    android:textAllCaps="true"/>
                                <View
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/colorGrey"/>


                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:padding="3dp"
                                    android:gravity="center"
                                    android:textSize="@dimen/extra_small_size"
                                    android:textColor="@color/colorlightBlack"
                                    android:text="24" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_margin="5dp"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="match_parent"
                                android:orientation="vertical"
                                android:background="@drawable/bg_light_grey">

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorappPink"
                                    android:text="GENDER"
                                    android:padding="3dp"
                                    android:textSize="@dimen/extra_small_size"
                                    android:gravity="center"
                                    android:textAllCaps="true"/>
                                <View
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/colorGrey"/>

                                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:padding="3dp"
                                    android:gravity="center"
                                    android:textSize="@dimen/extra_small_size"
                                    android:textColor="@color/colorlightBlack"
                                    android:text="Male" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="5dp"
                            android:background="@drawable/bg_light_grey">
                            <LinearLayout
                                android:layout_margin="3dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <EditText
                                    android:layout_width="wrap_content"
                                    android:layout_height="40dp"
                                    android:hint="Notes:-"
                                    android:background="@android:color/transparent"
                                    android:layout_margin="3dp"
                                    android:paddingLeft="5dp"
                                    android:gravity="center_vertical"
                                    android:singleLine="true"
                                    android:layout_weight="1"
                                    android:textSize="@dimen/extra_small_size"/>
                                <com.tabyq.pos.tabqypos.customtext.CustomTextButtonTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Add"
                                    android:layout_margin="2dp"
                                    android:layout_gravity="center"
                                    android:padding="8dp"
                                    android:textColor="@color/colorWhite"
                                    android:background="@drawable/btn_backgroundpink"/>
                            </LinearLayout>

                            <View
                                android:layout_marginTop="6dp"
                                android:layout_marginBottom="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorGrey"/>

                        </LinearLayout>
                    </LinearLayout>

                </android.support.v7.widget.CardView>

            </LinearLayout>


            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="2dp"
                android:layout_margin="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <com.tabyq.pos.tabqypos.customtext.CustomTextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Past Orders"
                        android:padding="8dp"
                        android:textColor="@color/colorBlack"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#F2F3F8"
                        android:orientation="horizontal">


                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:textSize="10sp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="Order Number"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_no"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:text="Date and Time"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_date"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Type"
                            android:textSize="10sp"
                            android:gravity="center"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_walkin"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Time Taken"
                            android:padding="5dp"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:id="@+id/item_save_past_order_time_taken"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Amount"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_amt"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textSize="10sp"
                            android:text="Status"
                            android:gravity="center"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_status"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="Driver"
                            android:textSize="10sp"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_driver"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Agent"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:padding="5dp"
                            android:id="@+id/item_save_past_order_agent"
                            android:textColor="@color/colorappPink"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:padding="5dp"
                            android:textColor="@color/colorWhite"/>
                    </LinearLayout>

                    <android.support.v7.widget.RecyclerView
