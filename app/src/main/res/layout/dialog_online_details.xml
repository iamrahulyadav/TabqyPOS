<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="700dp"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:background="@drawable/shape_border_grey_pop_up">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4.7"
                android:orientation="vertical"
                android:background="@drawable/shape_background_pop_up_white"
                android:padding="20dp">

                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dialog_online_details_customer_name"
                    android:textSize="@dimen/size_12"/>
                <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Munir"
                    android:textSize="@dimen/size_12"
                    android:layout_marginTop="@dimen/size_10"
                    android:textColor="@color/colorlightBlack"/>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10">

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dialog_online_details_customer_phone"
                            android:textSize="@dimen/size_12"
                            android:layout_alignParentLeft="true"/>

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dialog_online_details_customer_phone"
                            android:textSize="@dimen/size_12"
                            android:layout_alignParentRight="true"/>

                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp">

                    <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="05501252"
                        android:textSize="@dimen/size_12"
                        android:textColor="@color/colorBlack"
                        android:layout_alignParentLeft="true"/>

                    <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="27"
                        android:textSize="@dimen/size_12"
                        android:textColor="@color/colorBlack"
                        android:layout_alignParentRight="true"/>


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10">

                    <com.tabyq.pos.tabqypos.customtext.CustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dialog_online_details_customer_area"
                        android:textSize="@dimen/size_12"
                        android:layout_alignParentLeft="true"/>

                    <com.tabyq.pos.tabqypos.customtext.CustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dialog_online_details_customer_loc"
                        android:textSize="@dimen/size_12"
                        android:layout_alignParentRight="true"/>

                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp">

                    <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="As Sulimaniya"
                        android:textSize="@dimen/size_12"
                        android:textColor="@color/colorBlack"
                        android:layout_alignParentLeft="true"/>

                    <com.tabyq.pos.tabqypos.customtext.CustomTextViewBold
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prince Suliman\nRoad"
                        android:gravity="right"
                        android:textSize="@dimen/size_12"
                        android:textColor="@color/colorBlack"
                        android:layout_alignParentRight="true"/>


                </RelativeLayout>

                <com.tabyq.pos.tabqypos.customtext.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dialog_online_details_customer_del_add"
                    android:textSize="@dimen/size_12"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"/>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <com.tabyq.pos.tabqypos.customtext.CustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/dialog_online_cross"
                        android:text="@string/dialog_online_details_close"
                        android:textSize="@dimen/size_12"
                        android:textColor="@color/colorWhite"
                        android:drawableLeft="@drawable/ic_cross_white"
                        android:drawablePadding="10dp"
                        android:paddingLeft="18sp"
                        android:paddingRight="18sp"
                        android:paddingBottom="7sp"
                        android:paddingTop="7sp"
                        android:background="@drawable/shape_background_pink"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:drawableStart="@drawable/ic_cross_white"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_margin="10dp"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="5.3"
                android:background="@drawable/shape_background_pop_up_white"
                android:paddingTop="@dimen/size_20"
                android:paddingBottom="@dimen/size_20">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/dialog_online_details_right_top"
                        android:background="@color/colorWhite"
                        android:orientation="horizontal"
                        android:layout_alignParentTop="true"
                        android:paddingLeft="@dimen/size_20"
                        android:paddingRight="@dimen/size_20">

                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/extra_small_size"
                            android:layout_weight="1"
                            android:textColor="@color/colorappPink"
                            android:text="@string/fragment_crm_items_name"
                            android:gravity="left"/>
                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="@color/colorappPink"
                            android:textSize="@dimen/extra_small_size"
                            android:layout_weight="1"
                            android:text="@string/fragment_crm_qty" />
                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:textColor="@color/colorappPink"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textSize="@dimen/extra_small_size"
                            android:layout_weight="1"
                            android:text="@string/fragment_crm_price" />
                        <com.tabyq.pos.tabqypos.customtext.CustomTextView
                            android:layout_width="0dp"
                            android:textColor="@color/colorappPink"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:textSize="@dimen/extra_small_size"
                            android:layout_weight="1"
                            android:text="@string/fragment_crm_subtotal" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_below="@+id/dialog_online_details_right_top"
                        android:layout_above="@+id/dialog_online_details_right_bottom"
                        android:layout_marginTop="@dimen/size_10">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height=".3dp"
                            android:background="#ECF0F1"
                            android:layout_gravity="top"/>
                        <android.support.v7.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/dialog_online_details_recycler" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/dialog_online_details_right_bottom"
                        android:orientation="vertical"
                        android:layout_alignParentBottom="true">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height=".3dp"
                            android:background="#ECF0F1" />
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:paddingLeft="@dimen/size_20"
                            android:paddingRight="@dimen/size_20"
                            android:paddingTop="@dimen/size_10"
                            android:paddingBottom="@dimen/size_10">

                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:gravity="start"
                                android:text="@string/fragment_charge_txtsubtotal"
                                android:textColor="@color/colorDarkGrey"
                                android:textSize="@dimen/extra_small_size"
                                android:layout_alignParentLeft="true"/>
                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:id="@+id/walkin_sub_total"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:text="75.00"
                                android:layout_alignParentRight="true"
                                android:textColor="@color/colorDarkGrey"
                                android:textSize="@dimen/extra_small_size" />

                        </RelativeLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height=".3dp"
                            android:background="#ECF0F1" />
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/size_10"
                            android:paddingTop="@dimen/size_10"
                            android:paddingRight="@dimen/size_20"
                            android:paddingLeft="@dimen/size_20">
                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/extra_small_size"
                                android:text="@string/fragment_charge_txttax"
                                android:layout_alignParentLeft="true"
                                android:textColor="@color/colorDarkGrey" />
                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:textSize="@dimen/extra_small_size"
                                android:layout_height="wrap_content"
                                android:text="40.00"
                                android:layout_alignParentRight="true"
                                android:textColor="@color/colorDarkGrey" />

                        </RelativeLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height=".3dp"
                            android:background="#ECF0F1" />
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="@dimen/size_20"
                            android:paddingRight="@dimen/size_20"
                            android:paddingTop="@dimen/size_10"
                            android:paddingBottom="@dimen/size_10">

                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/small_size"
                                android:text="@string/fragment_charge_txttotal"
                                android:textColor="@color/colorBlack"
                                android:layout_alignParentLeft="true"/>
                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/small_size"
                                android:layout_alignParentRight="true"
                                android:text="SAR 75.00"
                                android:textColor="@color/colorBlack" />

                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="@dimen/size_20"
                            android:paddingRight="@dimen/size_20">

                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dialog_online_details_re_print"
                                android:textSize="@dimen/size_12"
                                android:textColor="@color/colorWhite"
                                android:paddingLeft="18sp"
                                android:paddingRight="18sp"
                                android:paddingBottom="7sp"
                                android:paddingTop="7sp"
                                android:background="@drawable/shape_background_pink"
                                android:layout_alignParentLeft="true"/>
                            <com.tabyq.pos.tabqypos.customtext.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dialog_online_details_status_history"
                                android:textSize="@dimen/size_12"
                                android:textColor="@color/colorWhite"
                                android:paddingLeft="18sp"
                                android:paddingRight="18sp"
                                android:paddingBottom="7sp"
                                android:paddingTop="7sp"
                                android:background="@drawable/shape_background_pink"
                                android:layout_alignParentRight="true" />

                        </RelativeLayout>

                    </LinearLayout>

                </RelativeLayout>


            </LinearLayout>

        </LinearLayout>

</LinearLayout>